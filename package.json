{
  "name": "ang-quickstart",
  "version": "1.0.0",
  "description": "Ekstremedia Angular Quickstart",
  "main": "index.js",
  "scripts": {
    "startdev": "webpack-dev-server --progress --colors",
    "start": "node server",
    "startboth": "node server && webpack-dev-server --progress --colors",
    "build": "webpack --progress --colors --config webpack.prod.config.js",
    "min": "webpack --progress --colors --optimize-minimize --optimize-occurence-order --optimize-dedupe --config dist.min.config.js",
    "both": "webpack --progress --colors --config webpack.prod.config.js && webpack --progress --colors --optimize-minimize --optimize-occurence-order --optimize-dedupe --config dist.min.config.js"
  },
  "author": "Terje Nesthus",
  "license": "WTFPL",
  "dependencies": {
    "angular": "^1.4.9",
    "angular-animate": "^1.5.5",
    "angular-moment": "^1.0.0-beta.6",
    "angular-ui-bootstrap": "^1.3.2",
    "angular-ui-router": "^0.2.17",
    "body-parser": "^1.14.2",
    "bootstrap-sass": "^3.3.6",
    "gsap": "^1.18.4",
    "jquery": "^2.2.4",
    "lodash": "^4.1.0",
    "moment": "^2.13.0",
    "normalize.css": "^4.1.1"
  },
  "devDependencies": {
    "angular-images-loaded": "^0.2.0",
    "autoprefixer-loader": "^3.2.0",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "bootstrap-loader": "^1.0.7",
    "css-loader": "^0.23.1",
    "debug": "^2.2.0",
    "file-loader": "^0.8.5",
    "imports-loader": "^0.6.5",
    "node-sass": "^3.4.2",
    "raw-loader": "^0.5.1",
    "resolve-url-loader": "^1.4.3",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.12",
    "webpack-dev-server": "^1.14.1"
  }
}
